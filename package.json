{
  "name": "zimara-ui",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=14.x"
  },
  "scripts": {
    "start": "webpack serve --hot",
    "build": "webpack --mode production",
    "build-storybook": "build-storybook -s public",
    "chromatic": "CHROMATIC_STORYBOOK_VERSION='@storybook/react@6.2.0' chromatic --exit-zero-on-changes",
    "dev": "webpack serve --progress",
    "format": "prettier --check 'src'",
    "format:fix": "prettier --write --ignore-unknown 'src'",
    "lint": "eslint --ext .ts,.tsx,.js ./src",
    "lint:fix": "yarn lint --fix",
    "lint-staged": "yarn lint:fix && yarn format:fix",
    "storybook": "start-storybook -p 6006 -s public",
    "test": "jest src/**/*.test.ts*",
    "test:coverage": "jest --coverage --watchAll=false",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@monaco-editor/react": "4.3.1",
    "@patternfly/patternfly": "4.151.3",
    "@patternfly/react-core": "4.166.0",
    "konva": "8.2.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-konva": "17.0.2-5",
    "react-router-dom": "5.3.0",
    "react-router-last-location": "2.0.1",
    "react-shadow": "19.0.2",
    "use-debounce": "7.0.0",
    "uuid": "8.3.2",
    "web-vitals": "2.1.2",
    "yarn": "2.4.3"
  },
  "devDependencies": {
    "@mdx-js/react": "1.6.22",
    "@patternfly/react-icons": "4.17.2",
    "@storybook/addon-a11y": "6.4.0-beta.20",
    "@storybook/addon-actions": "6.4.0-beta.20",
    "@storybook/addon-docs": "6.4.0-beta.20",
    "@storybook/addon-links": "6.4.0-beta.20",
    "@storybook/builder-webpack5": "6.3.12",
    "@storybook/manager-webpack5": "6.3.12",
    "@storybook/node-logger": "6.4.0-beta.20",
    "@storybook/preset-create-react-app": "4.0",
    "@storybook/react": "6.4.0-beta.20",
    "@testing-library/dom": "^8.10.1",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "12.1.2",
    "@testing-library/user-event": "13.5.0",
    "@types/jest": "27.0.2",
    "@types/react": "^17.0.33",
    "@types/react-router-dom": "5.3.2",
    "@types/uuid": "8.3.1",
    "@typescript-eslint/eslint-plugin": "5.2.0",
    "@typescript-eslint/parser": "4.33.0",
    "chromatic": "6.0.4",
    "css-loader": "6.4.0",
    "dotenv-webpack": "7.0.3",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.25.2",
    "eslint-plugin-jest": "25.2.2",
    "eslint-plugin-react": "7.26.1",
    "eslint-plugin-react-hooks": "4.2.0",
    "html-webpack-plugin": "5.5.0",
    "jest": "27.3.1",
    "monaco-editor": "0.29.1",
    "node-sass": "6.0.1",
    "prettier": "2.4.1",
    "prop-types": "15.7.2",
    "source-map-loader": "3.0.0",
    "style-loader": "3.3.1",
    "ts-jest": "27.0.7",
    "ts-loader": "9.2.6",
    "tsconfig-paths-webpack-plugin": "3.5.1",
    "tslib": "^2.3.1",
    "typescript": "4.4.4",
    "webpack": "5.60.0",
    "webpack-cli": "4.9.1",
    "webpack-dev-server": "4.3.1",
    "webpack-merge": "5.8.0",
    "yaml-loader": "0.6.0"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{tsx,ts}": "prettier --write",
    "*.tsx": "eslint --ext .tsx"
  },
  "packageManager": "yarn@3.1.0"
}
